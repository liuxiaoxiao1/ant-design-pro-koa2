  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register",
    "//": "下面是迁移React项目的脚本",
    "precommit": "npm run lint-staged",
    "react-start": "cross-env DISABLE_ESLINT=true roadhog dev",
